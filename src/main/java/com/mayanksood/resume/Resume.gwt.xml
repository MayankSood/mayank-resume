<module rename-to="resume">
    <inherits name="com.google.gwt.user.User"/>

    <inherits name='com.googlecode.mgwt.mvp.MVP'/>

    <inherits name="com.googlecode.mgwt.MGWT" />

    <entry-point class="com.mayanksood.resume.client.ResumeEntryPoint"/>

    <replace-with class="com.mayanksood.resume.client.DefaultClientFactory">
        <when-type-is class="com.mayanksood.resume.client.ClientFactory"/>
    </replace-with>

    <collapse-all-properties value="true"/>

    <set-property name="user.agent" value="gecko1_8,safari"/>

    <inherits name="com.google.gwt.logging.Logging"/>
    <set-property name="gwt.logging.enabled" value="TRUE"/>
    <set-property name="gwt.logging.logLevel" value="ALL"/>

    <source path="client"/>
</module>
